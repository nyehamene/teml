// keyword

package: "package";
import: "import";
using: "using";
component: "component";
page: "page";

// basic

identifier: /[a-zA-Z][a-zA-Z_-]*/;
string: string_quoted | stirng_line;
string_quoted: /"/ /[^"]*/ /"/; 
string_line: "-" "-" /[^\n]*/;

// reserved

model: "reserved";
true: "true";
false: "false";
nil: "nil";
text: "text";
